; Binary Search Trees
(define-struct no-info [])
(define NONE (make-no-info))

(define-struct node [ssn name left right])
; A BT (short for BinaryTree) is one of:
; - NONE
; - (make-node Number Symbol BT BT)

(make-node 15 'd NONE (make-node 24 'i NONE NONE))

